    "@types/hast": "^3.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/mdast": "^4.0.4",
    "@vitest/coverage-v8": "^1.6.0",
    "dedent": "^1.5.3",
    "jsdom": "^24.1.0",
    "jsr": "^0.12.4",
    "lefthook": "^1.6.18",
